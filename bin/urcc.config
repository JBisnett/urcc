[CC]
cc = clang
version = 3.4
llvm_path = /usr/lib/llvm-3.4/bin
clang_path = /usr/bin/clang
opt_path = /usr/bin/opt


[OUTPUT]
# decide the output of the program

# unopted version of IR file
unopt_ir = true

# unopted version of C dump
unopt_cdump = true

# unopted version of compiled bin
unopt_bin = true

# opted version of IR file
opt_ir = true

# unopted version of C dump
opt_cdump = true

# unopted version of compiled bin
opt_bin = true


[PASS]
pass_path = .